{
  "name": "contactkeeper",
  "version": "1.0.0",
  "description": "A contacts managment application.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix ../client",
    "client-install": "npm i --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
